  <script src="..%LWeb_dirMenu%/assets/bootstrap-4.3.1/dist/js/bootstrap.min.js"></script>
  <script src="..%LWeb_dirMenu%/assets/popper.min.js"></script>
  <script src="..%LWeb_dirMenu%/assets/jquery.nicescroll.min.js"></script>
  <script src="..%LWeb_dirMenu%/assets/moment.min.js"></script>
  <script src="..%LWeb_dirMenu%/assets/js/stisla.js"></script>

  <!-- JS Libraies -->

  <!-- Page Specific JS File -->

  <!-- Template JS File -->
  <script src="..%LWeb_dirMenu%/assets/js/scripts.js"></script>
  <script src="..%LWeb_dirMenu%/assets/js/custom.js"></script>
</body>
</html>