      </tr>
      <tr height=14pt>
       <td>Deduction (Out)</td>
       <td width="150"><%@LW_HField_er_qty_o@%><%@LW_HField_er_amt_o@%>
      </tr>
      <tr height=14pt>
       <td>Balance</td>
       <td width="150"><%@LW_HField_er_qty_q +er_qty_i -er_qty_o@%><%@LW_HField_er_amt_q +er_amt_i -er_amt_o@%>
      </tr>
     </TABLE>

     <table>
      <td><input type=text name=LW_key size=33></td>
      <td><input type=text name=LW_keySort size=33></td>
     </table>
     <table><td></td></table>

     <iframe width=800 height=297 frameborder=yes src="..%LWeb_ifi%" id="form_head"></iframe>
     <TABLE cellpadding=0 cellspacing=0 style="table-layout:fixed; font-size:8pt; height:8; border-collapse:collapse">
      <td><button type="submit" class="button" name="LW_prgfun" value="PSW210_F5" title="Detail">&nbsp;&nbsp;&nbsp;<img class="image" src="..%LWeb_dirMenu%/img/crt.png"> Create/View Request&nbsp;&nbsp;</button></td>
     </table>
  %@LW_onload@%
  </form>
 </body>
</html>